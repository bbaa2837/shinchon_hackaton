<div class="text-center" style ="background-color: #ffffff">
  <br>
  <h2 style = "color: #ff6d5e">로그인</h2>

  <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <!--<div class="form-inputs" style="margin:auto">-->
  <div class="asdasdasd">
    <%= f.input :email, required: false, autofocus: true, value:"이메일"%>
    <%= f.input :password, required: false, value:"비밀번호"%>
    <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
  </div>

  <!--<div class="form-actions">-->
  <div class="asdasdasd">
    <%= f.button :submit, "확인" %>
  </div>
</div>
<% end %>

<%= render "devise/shared/links" %>
